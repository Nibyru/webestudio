<section id="bottom">
  <div id="map">
    <iframe width="1900" height="425" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.com.ar/maps/ms?msa=0&amp;msid=209634190358231717209.0004e53504b97c44d602e&amp;gl=ar&amp;ie=UTF8&amp;t=m&amp;ll=-24.190332,-65.299194&amp;spn=0.008319,0.04077&amp;z=16&amp;output=embed"></iframe>
  </div>
  <div class="container" style="margin-top: 20px;">
    <div class="row margin-40">
      <div class="span10 offset1 text-center">
        <p><%= @content.direction %>, Jujuy, Argentina | <%= @content.telephone %>  |  <a href="mailto:<%= @content.email %>"><i class="icon-envelope-alt"></i> <%= @content.email %></a></p>
        <hr>
      </div>
    </div>
    
    <div class="row">
      <div class="span10 offset1 text-center">
        <!--Social Icons-->
        <ul class="social-icons">
          <li><a class="twitter" href="<%= @content.twitter %>" target="_blank"><i class="icon-twitter icon-3x"></i></a></li>
          <li><a class="facebook" href="<%= @content.facebook %>" target="_blank"><i class="icon-facebook icon-3x"></i></a></li>
          <li><a class="linkedin" href="<%= @content.facebook %>" target="_blank"><i class="icon-linkedin icon-3x"></i></a></li>
        </ul>
      </div>
    </div>
    
  </div>
</section>


<section id="footer">
  <div class="container">
    <div class="row">
      <div class="span12 text-center">
        <p>Created by <a href="http://www.nibyru.com/">Nibyru.</a></p>
      </div>
    </div>
  </div>
</section>

<a href="#top" class="scrollToTop hidden-phone" style="display: none;"><i class="icon-chevron-up"></i></a>

<!-- Javascript
================================================== -->
<!-- Placed at the end of the document so the pages load faster -->
<%= javascript_include_tag 'jquery.min.js' %>
<script>window.jQuery || document.write('<script src="js/jquery-1.9.1.min.js"><\/script>')</script>
<%= javascript_include_tag "application" %>

<script>
$(document).ready(function(){
  $(function(){
    $('#header').carousel()
  });
  $(window).load(function() {
    $('.flexslider').flexslider({
      animation: "slide"
    });
  });
  $('.wysihtml5').each(function(i, elem) {
    $(elem).wysihtml5({
      "font-styles": false,
      "link": false,
      "image": false
    });
  });
})

</script>