<!-- Links -->
    <section id="links">
      <div class="container text-center">
        <div class="row margin-40">
        <% @links.each_with_index do |link, index| %>
        <% if ((index+1) % 3) == 0 %>
          <div class="row margin-40">
        <% end %>
          <div class="span4">
            <div class="thumbs">
              <%= image_tag(link.photo_url(:link)) if link.photo? %>
              <%#= image_tag('portfolio1.jpg', :alt => 'Image Size 430x280') %>
              <a href="<%= link.url %>" target="_blank">
                <span class="title">
                  <span><i class="icon-link icon-2x"></i></span>
                </span> 
              </a>
            </div>
            
            <div class="project-title margin-30">
              <h4><%= link.name %></h4>
            </div>
          </div>
            
          <% if ((index+1) % 3) == 0 %>
            </div>
          <% end %>
        <% end %>
        </div>
        
    </section>
